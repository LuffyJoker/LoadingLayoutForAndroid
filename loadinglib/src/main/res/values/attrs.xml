<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LoadingView">
        <attr name="attr_loadingView" format="reference" />
        <attr name="attr_emptyView" format="reference" />
        <attr name="attr_errorView" format="reference" />
        <attr name="attr_viewState" format="enum">
            <enum name="content" value="0" />
            <enum name="error" value="1" />
            <enum name="empty" value="2" />
            <enum name="loading" value="3" />
        </attr>
        <attr name="attr_animateViewChanges" format="boolean" />
    </declare-styleable>

</resources>